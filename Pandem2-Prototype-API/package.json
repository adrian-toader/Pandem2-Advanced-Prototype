    "@fastify/swagger-ui": "^1.9.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "assert": "^2.0.0",
    "async": "^3.2.4",
    "axios": "^1.3.6",
    "bcryptjs": "^2.4.3",
    "commander": "^10.0.1",
