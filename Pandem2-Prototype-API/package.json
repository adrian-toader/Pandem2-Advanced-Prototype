{
  "name": "pandem2-api",
  "version": "1.0.0",
  "description": "Pandem2 API",
  "repository": "https://github.com/ClarisoftTechnologies/Pandem2-Prototype-1-API",
  "main": "app.js",
  "scripts": {
    "lint": "eslint src/",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.build.json && npm run copyDefaultConfigFiles",
    "copyDefaultConfigFiles": "copyfiles -u 1 src/**/*.default dist/ && copyfiles -u 1 ./src/config/*.html dist/",
    "start": "node dist/app.js",
    "start:dev": "npm run clean && tsc-watch -p tsconfig.build.json --onSuccess \"node dist/app.js\"",
    "start:debug": "npm run clean && tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/app.js\"",
    "initDefaultUsers": "node dist/scripts/initDefaultUsers.js"
  },
